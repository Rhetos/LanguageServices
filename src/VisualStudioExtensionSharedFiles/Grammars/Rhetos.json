    "strings": {
      "patterns": [
        {
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "include": "source.sql"
            }
          ]
        },
        {
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "include": "$self"
